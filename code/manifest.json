{
  "manifest_version" : 2,
  "minimum_chrome_version" : "18",
  "version" : "0.1",
  "name": "Lazy Kanban",
  "description": "Impression au format Kanban.",
  "browser_action": {
    "name": "AC Kanban",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": [ "jquery.min.js","content.js"],
    "matches": ["https://projets.kiwi.ca/*"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions" : ["activeTab"]
}